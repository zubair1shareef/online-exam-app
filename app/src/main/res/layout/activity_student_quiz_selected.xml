    <TextView
        android:id="@+id/createby"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="71dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/Branch"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView19"
        app:layout_constraintTop_toBottomOf="@+id/Quiztitle" />

    <TextView
        android:id="@+id/Semister"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="71dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/Totalmarks"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView21"
        app:layout_constraintTop_toBottomOf="@+id/Branch" />

    <TextView
        android:id="@+id/Branch"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="71dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/Semister"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView20"
        app:layout_constraintTop_toBottomOf="@+id/createby" />

    <TextView
        android:id="@+id/Quiztitle"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="71dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/createby"
        app:layout_constraintEnd_toEndOf="parent"

<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.quizappcomplete">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Studentprofile"></activity>
        <activity android:name=".ProfessorViewResultActivity" />
        <activity android:name=".ProfessorViewQuizActivity" />
        <activity android:name=".StudentResultListActivity" />
        <activity
            android:name=".StudentQuizSelectedActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".StudentDashboardActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".Adminaddinst"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".Results"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".ProfessorViewQuestionsActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".WelcomeActivity"
            android:screenOrientation="fullSensor">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ProfessorDashboardActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".StudentProfileCompleteActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".ProfessorNewQuizActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".ProfessorAddQuestionActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".Quiz"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".Register"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="fullSensor"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>